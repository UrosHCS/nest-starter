{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug-remote": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register 'test/**/*spec.ts'",
    "test:only": "NODE_ENV=test mocha -r ts-node/register -r tsconfig-paths/register",
    "test:watch": "node -p \"'TODO'\"",
    "test:debug": "node -p \"'TODO'\"",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "seed": "node ./scripts/seed.js",
    "pinker": "node --experimental-repl-await -r ./scripts/pinker.js",
    "tspinker": "ts-node -r tsconfig-paths/register -r ./scripts/pinker.ts",
    "pinker-from-src": "node --experimental-repl-await -r ts-node/register -r tsconfig-paths/register -r ./scripts/pinker.ts"
  },
  "dependencies": {
    "@nestjs/common": "^7.3.2",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "^7.3.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/platform-fastify": "^7.3.2",
    "@nestjs/platform-ws": "^7.3.2",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjs/websockets": "^7.3.2",
    "@types/ws": "^7.2.6",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.12.2",
    "mysql": "^2.18.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.0",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1",
    "ws": "^7.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.4.1",
    "@nestjs/schematics": "^7.0.1",
    "@nestjs/testing": "^7.3.2",
    "@types/bcrypt": "^3.0.0",
    "@types/chai": "^4.2.12",
    "@types/faker": "^4.1.12",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.0.26",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.10",
    "axios": "^0.19.2",
    "chai": "^4.2.0",
    "mocha": "^8.1.3",
    "prettier": "^2.0.5",
    "sqlite3": "^5.0.0",
    "supertest": "^4.0.2",
    "ts-loader": "^8.0.1",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.9.7"
  }
}
