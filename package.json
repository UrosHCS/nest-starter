{
  "name": "nest-starter",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug-remote": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "typeorm": "node -r tsconfig-paths/register -r ts-node/register ./node_modules/typeorm/cli.js",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "seed": "node ./scripts/seed.js",
    "ts-seed": "ts-node -r tsconfig-paths/register ./scripts/seed.ts",
    "ard": "node --experimental-repl-await ./scripts/ard.js",
    "ts-ard": "node --experimental-repl-await -r tsconfig-paths/register -r ts-node/register ./scripts/ard.ts"
  },
  "dependencies": {
    "@nestjs/common": "^8.2.6",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "^8.2.6",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^8.2.6",
    "@nestjs/platform-ws": "^8.2.6",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/typeorm": "^8.0.3",
    "@nestjs/websockets": "^8.2.6",
    "@types/ws": "^8.2.2",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "csv-parse": "^5.0.4",
    "mysql": "^2.18.1",
    "passport": "^0.5.2",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.2",
    "sqlite3": "^4.2.0",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41",
    "ws": "^8.4.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^8.2.6",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^27.4.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.14",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "axios": "^0.25.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "typescript": "^4.5.5"
  },
  "volta": {
    "node": "16.13.2",
    "npm": "8.4.0"
  }
}
