{
    "editor.tabSize": 2,
    "search.exclude": {
        "**/node_modules": false,
        "**/bower_components": true,
        "**/*.code-search": true,
        "dist": true,
    },
    "[typescript]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit",
            "source.fixAll": "explicit"
        },
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.formatOnSave": true,
    },
}